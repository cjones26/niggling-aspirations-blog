{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": true
  },
  "exclude": ["node_modules", "public", ".cache"]
}
